.chart svg
  height 500px;

.chart g.nv-scatter g.nv-series-0 .nv-point
	fill-opacity: 1 !important;
	stroke-opacity: 1 !important;

.chart g.nv-series-0 path.nv-line
    stroke-opacity: 0;

.point
  fill: #72A2EB;
  stroke: #020E9E;


.point.selected
  fill: #DB69EB;
  fill-opacity: 1;
  stroke: brown;

.brush .extent
  stroke: #fff;
  fill-opacity: .125;
  shape-rendering: crispEdges;

.best-fit-line
  stroke: #00D6FF;
  stroke-width: 3;
  fill: none;